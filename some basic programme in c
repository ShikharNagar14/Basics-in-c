// Q14 Factorial of a given number
/*
#include <stdio.h>

int main()
{
    int number, factorial = 1, i = 1;
    printf("Enter the number ");
    scanf("%d", &number);
    while (i < (number + 1))
    {
        factorial = factorial * i;
        i++;
    }
    printf("The factorial of %d is %d", number, factorial);

    return 0;
