int main(){
  int n,x;
  int i,j;
  printf("Enter the numbers\n");
  scanf("%d",&n);
  for(i=1;i<=n;i++){
    for(j=1;j<=10;j++){
      printf("%d*%3d-->%3d\n",i,j,i*j);
    }

    printf("\n");
  }

}
